{
  "name": "sverigesradio",
  "version": "1.0.1",
  "description": "CLI for streaming Sveriges Radio (P1, P2, P3)",
  "main": "dist/index.js",
  "bin": {
    "sverigesradio": "dist/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "nyc": "^11.4.0",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.3"
  },
  "nyc": {
    "report-dir": "test/coverage",
    "cache": true,
    "reporter": [
      "text",
      "html"
    ]
  },
  "scripts": {
    "start": "babel src -D -d dist --watch",
    "build": "babel src -D -d dist",
    "prepare": "npm run build",
    "test": "mocha --timeout 10000 --require babel-core/register",
    "coverage": "nyc --temp-directory ./test/coverage/tmp mocha --timeout 10000 --require babel-core/register",
    "coveralls": "nyc report --temp-directory ./test/coverage/tmp mocha --timeout 10000 --require babel-core/register --reporter=text-lcov | coveralls",
    "lint": "eslint src test --ignore-pattern test/coverage*"
  },
  "author": "Olle Lauri Bostr√∂m <ollebostr@gmail.com> (https://github.com/ollelauribostrom)",
  "license": "MIT",
  "dependencies": {
    "clear": "0.0.1",
    "commander": "^2.12.2",
    "lame": "^1.2.4",
    "ora": "^1.3.0",
    "request": "^2.83.0",
    "speaker": "^0.4.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ollelauribostrom/sverigesradio.git"
  },
  "keywords": [
    "radio",
    "sr",
    "p1",
    "p2",
    "p3",
    "sveriges radio",
    "stream",
    "mp3",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/ollelauribostrom/sverigesradio/issues"
  },
  "homepage": "https://github.com/ollelauribostrom/sverigesradio#readme"
}
